<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.mamia">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.MAMIA">
        <activity android:name=".InputSettingsActivity"
                  android:exported="true"
                  android:screenOrientation="fullSensor"
                  android:parentActivityName=".InputActivity">
        </activity>
        <activity android:name=".InputRecommendationsActivity"
                  android:exported="true"
                  android:screenOrientation="fullSensor"
                  android:parentActivityName=".InputActivity">
        </activity>
        <activity android:name=".InputDashboardActivity"
                  android:exported="true"
                  android:screenOrientation="fullSensor"
                  android:parentActivityName=".InputActivity">
        </activity>
        <activity android:name=".InputJournalActivity"
                  android:exported="true"
                  android:screenOrientation="fullSensor"
                  android:parentActivityName=".InputActivity">
        </activity>
        <activity
                android:name=".InputActivity"
                android:exported="true"
                android:screenOrientation="fullSensor"
                android:parentActivityName=".IntroActivity4">
        </activity>
        <activity
                android:name=".IntroActivity4"
                android:exported="true"
                android:screenOrientation="fullSensor"
                android:parentActivityName=".IntroActivity3">
        </activity>
        <activity
                android:name=".IntroActivity3"
                android:exported="true"
                android:screenOrientation="fullSensor"
                android:parentActivityName=".IntroActivity2">
        </activity>
        <activity
                android:name=".IntroActivity2"
                android:exported="true"
                android:screenOrientation="fullSensor"
                android:parentActivityName=".IntroActivity1">
        </activity>
        <activity
                android:name=".IntroActivity1"
                android:exported="true"
                android:screenOrientation="fullSensor"
                android:parentActivityName=".MainActivity">
        </activity>
        <activity
                android:name=".MainActivity"
                android:exported="true"
                android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>